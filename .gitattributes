# git-crypt

# Browser extensions
"/browser-ext/**/*" filter=git-crypt diff=git-crypt

# FileZilla
"/apps/FileZilla/config/*.xml" filter=git-crypt diff=git-crypt

# Git
"/dots/git/.gitconfig" filter=git-crypt diff=git-crypt

# OBS Studio
"/apps/OBS Studio/obs-studio/basic/profiles/**/*/basic.ini" filter=git-crypt diff=git-crypt

# qBittorrent
"/apps/qBittorrent/**/*" filter=git-crypt diff=git-crypt
